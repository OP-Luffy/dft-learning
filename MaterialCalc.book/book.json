{
	"title": "Computation-Simulation",
	"description": "Notes to record the learning process of 'Calculation and simulation method of environmental energy materials'", 
	"author": "Northword",
	"language": "zh-hans",
	"gitbook": "3.2.3",
	"root": "./",
	"structure": {
        "readme": "INTRO.md"
    },
	"plugins": [
		"-highlight",
		"-ace",
		"-lunr",
		"-search",
		"-sharing",
		"livereload",
		"-expandable-chapters",
		"-page-treeview",
		"-anchor-navigation-ex",
		"chapter-fold",
		"code",
		"search-plus",
		"-splitter",
		"-mermaid-gb3",
		"popup",
		"-auto-scroll-table",
		"-theme-fexa",
		"theme-comscore",
		"-back-to-top-button",
		"-copy-code-button",
		"-cuav-chapters",
		"-heading-anchors",
		"theme-cap"
	],
	"pluginsConfig": {
		"theme-default": {
            "showLevel": false
        },
		"theme-cap":{
            "search-placeholder":"输入关键字搜索", 
            "logo":"./logo.png", 
            "favicon": "./fav.ico"
        }
	},
	"variables": {
		"themeCAP":{
            "nav":[
                {
                    "url":"../",
                    "target":"_blank",
                    "name": "Blog"
                }
            ]
        }
    }
}
