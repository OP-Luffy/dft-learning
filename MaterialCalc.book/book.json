{
	"title": "Computation-Simulation",
	"description": "Notes to record the learning process of 'Calculation and simulation method of environmental energy materials'", 
	"author": "Northword",
	"language": "zh-hans",
	"gitbook": "3.2.3",
	"root": "./",
	"structure": {
        "readme": "INTRO.md"
    },
	"plugins": [
		"-highlight",
		"prism",
		"prism-themes",
		"code",

		"-lunr",
		"-search",
		"search-plus",

		"-sharing",
		"livereload",
		
		"-expandable-chapters",
		"-page-treeview",
		"anchor-navigation-ex",
		"chapter-fold",
		"-splitter",
		
		"-mermaid-gb3",
		"-popup",
		"-auto-scroll-table",
		
		"-theme-fexa",
		"theme-comscore",
		"-theme-cap",
		"theme-northword1",
		
		"-back-to-top-button"
	],
	"pluginsConfig": {
		"theme-default": {
            "showLevel": false
        },
		"theme-cap":{
            "search-placeholder":"输入关键字搜索", 
            "logo":"./logo.png", 
            "favicon": "./fav.ico"
        },
		"anchor-navigation-ex":{
			"showLevel": false,
			"associatedWithSummary": true,
			"printLog": false,
			"multipleH1": false,
			"mode": "float",
			"showGoTop":true,
			"float": {
				"floatIcon": "fa fa-navicon",
				"showLevelIcon": false,
				"level1Icon": "fa fa-hand-o-right",
				"level2Icon": "fa fa-hand-o-right",
				"level3Icon": "fa fa-hand-o-right"
			},
			"pageTop": {
				"showLevelIcon": false,
				"level1Icon": "fa fa-hand-o-right",
				"level2Icon": "fa fa-hand-o-right",
				"level3Icon": "fa fa-hand-o-right"
			}
		}
	},
	"variables": {
		"themeCAP":{
            "nav":[
                {
                    "url":"../",
                    "target":"_blank",
                    "name": "Blog"
                }
            ]
        }
    }
}
